.page{
  display: flex;
  height: 100%;
  width: 100%;
  flex-direction:column;
  align-items:center;
  justify-content:space-around;
}
.face{
  width: 30%;
  height: 21%;
  border-radius: 50%;
  border: 1px solid #e0e0e0;
}
.face>image{
  width: 100%;
  height:100%;
}
.userinfo{
  width: 90%;
  width: 30%;
  border: 1px solid #e0e0e0;
}
.getuser{
  width: 50%;
  height:100%;
  font-size: 13px;
}